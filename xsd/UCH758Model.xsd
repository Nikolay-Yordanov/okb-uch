<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2022 (x64) (http://www.altova.com) by Alexandre Levine (Progmatic Lab) -->
<xs:schema xmlns="http://www.ucbreport.ru/2021/UCH" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:vc="http://www.w3.org/2007/XMLSchema-versioning" targetNamespace="http://www.ucbreport.ru/2021/UCH" elementFormDefault="qualified" attributeFormDefault="unqualified" vc:minVersion="1.0">
	<xs:include schemaLocation="UCH758Ref.xsd"/>
	<xs:include schemaLocation="UCH758Types.xsd"/>
	<!--Данная схема содержит модель данных Кредитной Истории физического и юридического лиц, составленную по модели изложенной в Положении 758 ЦБ РФ.-->
	<!--Глобальные элементы служат в качестве справочной информации-->
	<!--Группы реализовывают соответствующие Разделы, Подразделы и Секции документа 758. В их аннотациях присутствуют ссылки на документ-->
	<!--Глобальные типы реализовывают соответствующие Блоки документа 758. В их аннотациях присутствуют ссылки на документ-->
	<!--Зачеркнутые элементы (maxOccurs=0) сейчас не используются, но приведены для сохранения структуры и улучшения видимости, а также проверок-->
	<!--В схеме элементы расположены в порядке, в котором они встречаются в Положении-758 и пронумерованы в аннотациях соответствующими номерами-->
	<!---->
	<xs:annotation>
		<xs:documentation>Раздел 2. Показатели кредитной информации физического лица</xs:documentation>
	</xs:annotation>
	<xs:complexType name="PersonCreditHistoryType">
		<xs:annotation>
			<xs:documentation>Кредитная история физического лица</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:group ref="PersonCreditHistoryTitular">
				<xs:annotation>
					<xs:documentation>2.1. Титульная часть кредитной истории</xs:documentation>
				</xs:annotation>
			</xs:group>
			<xs:group ref="PersonCreditHistoryMain">
				<xs:annotation>
					<xs:documentation>2.2. Основная часть кредитной истории</xs:documentation>
				</xs:annotation>
			</xs:group>
			<xs:group ref="CreditHistoryAdditional">
				<xs:annotation>
					<xs:documentation>2.3. Дополнительная (закрытая) часть кредитной истории</xs:documentation>
				</xs:annotation>
			</xs:group>
			<xs:group ref="CreditHistoryInformational">
				<xs:annotation>
					<xs:documentation>2.4. Информационная часть кредитной истории</xs:documentation>
				</xs:annotation>
			</xs:group>
		</xs:sequence>
	</xs:complexType>
	<xs:group name="PersonCreditHistory">
		<xs:annotation>
			<xs:documentation>2. Показатели кредитной информации физического лица</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:group ref="PersonCreditHistoryTitular">
				<xs:annotation>
					<xs:documentation>2.1. Титульная часть кредитной истории</xs:documentation>
				</xs:annotation>
			</xs:group>
			<xs:group ref="PersonCreditHistoryMain">
				<xs:annotation>
					<xs:documentation>2.2. Основная часть кредитной истории</xs:documentation>
				</xs:annotation>
			</xs:group>
			<xs:group ref="CreditHistoryAdditional" minOccurs="0" maxOccurs="0">
				<xs:annotation>
					<xs:documentation>2.3. Дополнительная (закрытая) часть кредитной истории</xs:documentation>
				</xs:annotation>
			</xs:group>
			<xs:group ref="CreditHistoryInformational">
				<xs:annotation>
					<xs:documentation>2.4. Информационная часть кредитной истории</xs:documentation>
				</xs:annotation>
			</xs:group>
		</xs:sequence>
	</xs:group>
	<xs:group name="PersonCreditHistoryTitular">
		<xs:annotation>
			<xs:documentation>2.1. Титульная часть кредитной истории</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:group ref="SubjectMain">
				<xs:annotation>
					<xs:documentation>Основные сведения о субъекте кредитной истории</xs:documentation>
				</xs:annotation>
			</xs:group>
		</xs:sequence>
	</xs:group>
	<xs:group name="SubjectMain">
		<xs:annotation>
			<xs:documentation>Основные сведения о субъекте кредитной истории</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="name" type="PersonNameType">
				<xs:annotation>
					<xs:documentation>Блок 1. Имя</xs:documentation>
					<xs:documentation>Обновляемый</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="prevName" type="PrevPersonNameType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Блок 2. Предыдущее имя</xs:documentation>
					<xs:documentation>Исторический</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="birthInfo" type="BirthInfoType">
				<xs:annotation>
					<xs:documentation>Блок 3. Дата и место рождения</xs:documentation>
					<xs:documentation>Обновляемый</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="id" type="PersonIdType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Блок 4. Документ, удостоверяющий личность</xs:documentation>
					<xs:documentation>Обновляемый</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="prevId" type="PrevPersonIdType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Блок 5. Документ, ранее удостоверявший личность</xs:documentation>
					<xs:documentation>Обновляемый</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="tax" type="TaxType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Блок 6. Номер налогоплательщика и регистрационный номер</xs:documentation>
					<xs:documentation>Обновляемый</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="socialId" type="SocialIdType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Блок 7. СНИЛС</xs:documentation>
					<xs:documentation/>
					<xs:documentation>Обновляемый</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:group>
	<xs:complexType name="PersonNameType">
		<xs:annotation>
			<xs:documentation>Блок 1. Имя</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="firstName" type="nameType">
				<xs:annotation>
					<xs:documentation>Фамилия</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="lastName" type="nameType">
				<xs:annotation>
					<xs:documentation>Имя</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="midName" type="nameType">
				<xs:annotation>
					<xs:documentation>Отчество</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PrevPersonNameType">
		<xs:annotation>
			<xs:documentation>Блок 2. Предыдущее имя</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="PersonNameType">
				<xs:sequence>
					<xs:element name="date" type="dateType">
						<xs:annotation>
							<xs:documentation>Дата выдачи документа с измененным именем</xs:documentation>
							<xs:appinfo>Допустимые значения: в случае если по показателю «Код документа» в блоке показателей, содержащем настоящий показатель, указано «21», дата должна быть позднее или равна дате рождения субъекта плюс 13 лет.</xs:appinfo>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="BirthInfoType">
		<xs:annotation>
			<xs:documentation>Блок 3. Дата и место рождения</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="birthDate" type="dateType">
				<xs:annotation>
					<xs:documentation>Дата рождения</xs:documentation>
					<xs:appinfo>Допустимые значения: ранее даты формирования кредитной информации, но не ранее, чем дата формирования кредитной информации минус 150 лет.</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="countryCode" type="countryCodeType">
				<xs:annotation>
					<xs:documentation>Код страны по ОКСМ</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="birthPlace" type="nameType">
				<xs:annotation>
					<xs:documentation>Место рождения</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PersonIdType">
		<xs:annotation>
			<xs:documentation>Блок 4. Документ, удостоверяющий личность; Блок 5. Документ, ранее удостоверявший личность</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="countryCode" type="countryCodeType">
				<xs:annotation>
					<xs:documentation>Код страны гражданства по ОКСМ</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="otherCountry" type="nameType">
				<xs:annotation>
					<xs:documentation>Наименование иной страны</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="idCode" type="idType">
				<xs:annotation>
					<xs:documentation>Код документа</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="otherId" type="otherNameType">
				<xs:annotation>
					<xs:documentation>Наименование иного документа</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="idSeries" type="seriesType">
				<xs:annotation>
					<xs:documentation>Серия документа</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="idNum" type="infoType">
				<xs:annotation>
					<xs:documentation>Номер документа</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="issueDate" type="dateType">
				<xs:annotation>
					<xs:documentation>Дата выдачи документа</xs:documentation>
					<xs:appinfo>Допустимые значения: в случае если по показателю «Код документа» в блоке показателей, содержащем настоящий показатель, указано «21», дата должна быть позднее или равна дате рождения плюс 13 лет и позднее даты выдачи предыдущего документа (при наличии).</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="issuer" type="infoType">
				<xs:annotation>
					<xs:documentation>Кем выдан документ</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deptCode" type="authorityCodeType">
				<xs:annotation>
					<xs:documentation>Код подразделения</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="endDate" type="dateType">
				<xs:annotation>
					<xs:documentation>Дата окончания срока действия документа</xs:documentation>
					<xs:appinfo>Допустимые значения: позднее даты выдачи документа, указанной в блоке показателей, содержащем настоящий показатель.</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PrevPersonIdType">
		<xs:annotation>
			<xs:documentation>Блок 5. Документ, ранее удостоверявший личность</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="PersonIdType"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="TaxType">
		<xs:annotation>
			<xs:documentation>Блок 6. (юр Блок 6) Номер налогоплательщика и регистрационный номер</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="taxCode" type="taxCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Код номера налогоплательщика</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxNum" type="taxNumType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Номер налогоплательщика</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="regNum" type="regNumType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Регистрационный номер</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SocialIdType">
		<xs:annotation>
			<xs:documentation>Блок 7. СНИЛС</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="id" type="socialIdType">
				<xs:annotation>
					<xs:documentation>СНИЛС</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:group name="PersonCreditHistoryMain">
		<xs:annotation>
			<xs:documentation>2.2. Основная часть кредитной истории</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:group ref="SubjectSpecial">
				<xs:annotation>
					<xs:documentation>Специальные сведения о субъекте кредитной истории</xs:documentation>
				</xs:annotation>
			</xs:group>
			<xs:group ref="Obligation">
				<xs:annotation>
					<xs:documentation>Сведения об обязательстве субъекта кредитной истории</xs:documentation>
				</xs:annotation>
			</xs:group>
			<xs:group ref="CreditHistoryOther">
				<xs:annotation>
					<xs:documentation>Отдельные блоки основной части кредитной истории</xs:documentation>
				</xs:annotation>
			</xs:group>
			<xs:group ref="AccompanyingInfo"/>
		</xs:sequence>
	</xs:group>
	<xs:group name="SubjectSpecial">
		<xs:annotation>
			<xs:documentation>Специальные сведения о субъекте кредитной истории</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="regAddress" type="AddressType">
				<xs:annotation>
					<xs:documentation>Блок 8. Регистрация физического лица по месту жительства или пребывания</xs:documentation>
					<xs:documentation>Обновляемый
</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="factAddress" type="AddressType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Блок 9. Фактическое место жительства</xs:documentation>
					<xs:documentation>Обновляемый</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="contact" type="ContactType">
				<xs:annotation>
					<xs:documentation>Блок 10. Контактные данные</xs:documentation>
					<xs:documentation/>
					<xs:documentation>Обновляемый</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="soleProprietor" type="SelfEmployType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Блок 11. Государственная регистрация в качестве индивидуального предпринимателя</xs:documentation>
					<xs:documentation>Обновляемый</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="legalCapacity" type="LegalCapacityType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Блок 12. Сведения о дееспособности</xs:documentation>
					<xs:documentation>Исторический
</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="bankruptcy" type="BankruptcyType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Блок 13. (юр Блок 6) Сведения по делу о несостоятельности (банкротстве)</xs:documentation>
					<xs:documentation>Исторический
</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fulfillment" type="FulfillmentType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Блок 14. Сведения о завершении расчетов с кредиторами и освобождении субъекта от исполнения обязательств в связи с банкротством</xs:documentation>
					<xs:documentation>Исторический</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="rating" type="RatingType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Блок 15. Индивидуальный рейтинг субъекта</xs:documentation>
					<xs:documentation>Исторический</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="score" type="ScoreType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Блок 16. (юр Блок 9) Кредитная оценка (скоринг)</xs:documentation>
					<xs:documentation>Исторический</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:group>
	<xs:complexType name="AddressType">
		<xs:annotation>
			<xs:documentation>Блок 8. Регистрация физического лица по месту жительства или пребывания</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="regAddrCode" type="addressRegType">
				<xs:annotation>
					<xs:documentation>Код адреса регистрации</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="postal" type="postalType">
				<xs:annotation>
					<xs:documentation>Почтовый индекс</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="countryCode" type="countryCodeType">
				<xs:annotation>
					<xs:documentation>Код страны по ОКСМ</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="otherCountry" type="nameType">
				<xs:annotation>
					<xs:documentation>Наименование иной страны</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="addrNum" type="addressFiasType">
				<xs:annotation>
					<xs:documentation>Номер адреса в ФИАС</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="locationCode" type="ocatoType">
				<xs:annotation>
					<xs:documentation>Код населенного пункта по ОКАТО</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="otherLocation" type="infoType">
				<xs:annotation>
					<xs:documentation>Иной населенный пункт</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="street" type="nameType">
				<xs:annotation>
					<xs:documentation>Улица</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="house" type="buildingType">
				<xs:annotation>
					<xs:documentation>Дом</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="estate" type="buildingType">
				<xs:annotation>
					<xs:documentation>Владение</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="block" type="buildingType">
				<xs:annotation>
					<xs:documentation>Корпус</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="building" type="buildingType">
				<xs:annotation>
					<xs:documentation>Строение</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="apartment" type="buildingType">
				<xs:annotation>
					<xs:documentation>Квартира</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="regDate" type="dateType">
				<xs:annotation>
					<xs:documentation>Дата регистрации</xs:documentation>
					<xs:appinfo>Допустимые значения: не ранее даты возникновения обязательства и не позднее даты формирования кредитной информации.</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="regAuthority" type="orgNameType">
				<xs:annotation>
					<xs:documentation>Наименование регистрирующего органа</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="regAuthorityCode" type="codeType">
				<xs:annotation>
					<xs:documentation>Код подразделения, осуществившего регистрацию</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FactAddressType">
		<xs:annotation>
			<xs:documentation>Блок 9. Фактическое место жительства</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="AddressType"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ContactType">
		<xs:annotation>
			<xs:documentation>Блок 10. Контактные данные</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="phone" type="phoneType">
				<xs:annotation>
					<xs:documentation>Номер телефона</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="phoneComment" type="infoType">
				<xs:annotation>
					<xs:documentation>Комментарий к номеру телефона</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="email" type="emailType">
				<xs:annotation>
