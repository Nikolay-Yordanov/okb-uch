<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2022 (x64) (http://www.altova.com) by Alexandre Levine (Progmatic Lab) -->
<xs:schema xmlns="http://www.ucbreport.ru/2021/UCH" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:vc="http://www.w3.org/2007/XMLSchema-versioning" targetNamespace="http://www.ucbreport.ru/2021/UCH" elementFormDefault="qualified" attributeFormDefault="unqualified" vc:minVersion="1.1">
	<xs:include schemaLocation="UCH758Model.xsd"/>
	<!--Данная схема содержит модель загрузки записей Кредитной физического и юридического лиц, составленную по Положению-758 ЦБ РФ и описывает все релевантные сценарии загрузки, описанные в этом документе-->
	<!--Схема импортирует схему с моделью данных по Положению-758, типы которого использованы в сценариях загрузки-->
	<!--Записи (RecordType) приходят в пакете (package) и содержат события (event) абстрактного типа EventType, в который должны подставляться данные реальных типов PersonEventType и OrgEventType-->
	<!--В схеме элементы расположены в порядке, в котором они встречаются в Положении-758 и пронумерованы в аннотациях соответствующими номерами-->
	<!--Раздел 5. События, вследствие которых формируется кредитная информация, и связанные с ними группы блоков показателей кредитной информации-->
	<xs:element name="package">
		<xs:annotation>
			<xs:documentation>Пакет данных</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="externalID">
					<xs:annotation>
						<xs:documentation>Исходящий регистрационный номер документа с кредитными историями</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="100"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="prevExternalID" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Исходящий регистрационный номер документа с кредитными историями с ранее не принятой кредитной информацией – для документа, содержащего кредитную информацию, которая ранее была не принята.

Заполняется в случае корректировки</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="100"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="createDate" type="dateType">
					<xs:annotation>
						<xs:documentation>Дата и время создания документа</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="totalRecords">
					<xs:annotation>
						<xs:documentation>Количество записей в пакете для сверки</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:int">
							<xs:maxInclusive value="999999999"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="totalSubjects">
					<xs:annotation>
						<xs:documentation>Количество субъектов в пакете для сверки (необязательный параметр)</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:int">
							<xs:maxInclusive value="999999999"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="source">
					<xs:annotation>
						<xs:documentation>Сведения о партнере</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:group ref="SourceInfo"/>
					</xs:complexType>
				</xs:element>
				<xs:element name="record" type="RecordType" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Запись с событием Кредитной Истории</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="RecordType">
		<xs:annotation>
			<xs:documentation>Запись в пакете</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="orderNum">
				<xs:annotation>
					<xs:documentation>Идентификатор записи по данным источника  (Порядковый номер записи в файле)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:maxInclusive value="9999999999"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="eventDate" type="dateType">
				<xs:annotation>
					<xs:documentation>Дата события</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="operationType">
				<xs:annotation>
					<xs:documentation>Тип операции</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="event" type="EventType">
				<xs:annotation>
					<xs:documentation>Событие Кредитной Истории. Для Физического Лица используется PersonEventType. Для Юридического Лица используется OrgEventType</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EventType" abstract="true">
		<xs:annotation>
			<xs:documentation>Событие Кредитной Истории (абстрактный базовый тип)</xs:documentation>
		</xs:annotation>
	</xs:complexType>
	<xs:complexType name="PersonEventType">
		<xs:annotation>
			<xs:documentation>Событие Кредитной Истории Физического Лица</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="EventType">
				<xs:group ref="PersonEvents"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="OrgEventType">
		<xs:annotation>
			<xs:documentation>Событие Кредитной Истории Юридического Лица</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="EventType">
				<xs:group ref="OrgEvents"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="InternalEventType">
		<xs:annotation>
			<xs:documentation>Внутреннее событие Кредитной - используется только Кредитным Бюро</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="EventType">
				<xs:group ref="InternalEvеnts"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:group name="PersonEvents">
		<xs:annotation>
			<xs:documentation>Группа всех событий загрузки информации</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:group ref="SubjectEvents"/>
				<xs:group ref="DealEvents"/>
			</xs:choice>
			<xs:element name="subject">
				<xs:annotation>
					<xs:documentation>Титульная часть КИ Субъекта, используется только для поиска Субъекта</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:group ref="SubjectMain"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:group>
	<xs:group name="SubjectEvents">
		<xs:annotation>
			<xs:documentation>Группа событий по субъекту</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="subjectApplied" type="SubjectAppliedType">
				<xs:annotation>
					<xs:documentation>1.1 Субъект обратился к источнику с предложением совершить сделку</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="applicationChanged" type="ApplicationChangedType">
				<xs:annotation>
					<xs:documentation>1.2 Источник одобрил обращение субъекта (направил ему оферту) или изменились сведения об обращении</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="applicationRejected" type="ApplicationRejectedType">
				<xs:annotation>
					<xs:documentation>1.3 Источник отказался от совершения сделки по обращению субъекта</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="monetaryDealMade" type="MonetaryDealMadeType">
				<xs:annotation>
					<xs:documentation>1.4 Субъект и источник совершили сделку, кроме договора лизинга и поручительства по лизингу для денежного обязательства субъекта</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="sourceNonMonetaryDealMade" type="SourceNonMonetaryDealMadeType">
				<xs:annotation>
					<xs:documentation>1.4 Субъект и источник совершили сделку, кроме договора лизинга и поручительства по лизингу для неденежного обязательства источника</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="nonMonetaryDealMade" type="NonMonetaryDealMadeType">
				<xs:annotation>
					<xs:documentation>1.4 Субъект и источник совершили сделку, кроме договора лизинга и поручительства по лизингу для неденежного обязательства субъекта  </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="leasingDealMade" type="LeasingDealMadeType">
				<xs:annotation>
					<xs:documentation>1.4.1 Субъект и источник заключили договор лизинга либо поручительства по лизингу и предмет лизинга передан лизингополучателю</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="debtClaimSubmitted" type="DebtClaimSubmittedType">
				<xs:annotation>
					<xs:documentation>1.5 Для принудительного исполнения передано требование о взыскании долга по алиментам, платы за жилое помещение, коммунальные услуги или услуги связи</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="debtClaimChanged" type="DebtClaimChangedType">
				<xs:annotation>
					<xs:documentation>1.6 Изменились сведения о взыскании долга по алиментам, платы за жилое помещение, коммунальные услуги или услуги связи либо указанное требование погашено</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="subjectMainChanged" type="SubjectMainChangedType">
				<xs:annotation>
					<xs:documentation>1.7 Изменились сведения титульной части кредитной истории субъекта</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="subjectSpecialChanged" type="SubjectSpecialChangedType">
				<xs:annotation>
					<xs:documentation>1.9 Изменились сведения о субъекте в основной части кредитной истории, кроме сведений о дееспособности, банкротстве, индивидуальном рейтинге и кредитной оценке</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="subjectCapacityChanged" type="SubjectCapacityChangedType">
				<xs:annotation>
					<xs:documentation>1.10 Изменились сведения о дееспособности субъекта</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="subjectBankruptcyChanged" type="SubjectBankruptcyChangedType">
				<xs:annotation>
					<xs:documentation>1.12 Изменились сведения по делу о банкротстве субъекта</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
	</xs:group>
	<xs:complexType name="SubjectAppliedType">
		<xs:annotation>
			<xs:documentation>1.1 Субъект обратился к источнику с предложением совершить сделку</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="application" type="ApplicationType">
				<xs:annotation>
					<xs:documentation>Блок 55. (юр Блок 45) Сведения об обращении субъекта к источнику с предложением совершить сделку</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ApplicationChangedType">
		<xs:annotation>
			<xs:documentation>1.2 Источник одобрил обращение субъекта (направил ему оферту) или изменились сведения об обращении</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="application" type="ApplicationType">
				<xs:annotation>
					<xs:documentation>Блок 55. (юр Блок 45) Сведения об обращении субъекта к источнику с предложением совершить сделку</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ApplicationRejectedType">
		<xs:annotation>
			<xs:documentation>1.3 Источник отказался от совершения сделки по обращению субъекта</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="application" type="ApplicationType">
				<xs:annotation>
					<xs:documentation>Блок 55. (юр Блок 45) Сведения об обращении субъекта к источнику с предложением совершить сделку</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="appReject" type="AppRejectType">
				<xs:annotation>
					<xs:documentation>Блок 57. (юр Блок 47) Сведения об отказе источника от предложения совершить сделку</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="MonetaryDealMadeType">
		<xs:annotation>
			<xs:documentation>1.4 Субъект и источник совершили сделку, кроме договора лизинга и поручительства по лизингу для денежного обязательства субъекта</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:group ref="DealMade"/>
			<xs:element name="amount" type="DealAmountType">
				<xs:annotation>
					<xs:documentation>Блок 19. (юр Блок 12) Сумма и валюта обязательства</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="paymentTerms" type="PaymentTermsType">
				<xs:annotation>
					<xs:documentation>Блок 21. (юр Блок 14) Сведения об условиях платежей</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SourceNonMonetaryDealMadeType">
		<xs:annotation>
			<xs:documentation>1.4 Субъект и источник совершили сделку, кроме договора лизинга и поручительства по лизингу для неденежного обязательства источника</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:group ref="DealMade"/>
			<xs:element name="sourceNonMonetaryObligation" type="SourceNonMonetaryObligationType">
				<xs:annotation>
					<xs:documentation>Блок 30. (юр Блок 21) Сведения о неденежном обязательстве источника</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="NonMonetaryDealMadeType">
		<xs:annotation>
			<xs:documentation>1.4 Субъект и источник совершили сделку, кроме договора лизинга и поручительства по лизингу для неденежного обязательства субъекта  </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:group ref="DealMade"/>
			<xs:element name="subjectNonMonetaryObligation" type="SubjectNonMonetaryObligationType">
				<xs:annotation>
					<xs:documentation>Блок 31. (юр Блок 22) Сведения о неденежном обязательстве субъекта</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="LeasingDealMadeType">
		<xs:annotation>
			<xs:documentation>1.4.1 Субъект и источник заключили договор лизинга либо поручительства по лизингу и предмет лизинга передан лизингополучателю</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:group ref="DealMade"/>
			<xs:element name="amount" type="DealAmountType">
				<xs:annotation>
					<xs:documentation>Блок 19. (юр Блок 12) Сумма и валюта обязательства</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="paymentTerms" type="PaymentTermsType">
				<xs:annotation>
					<xs:documentation>Блок 21. (юр Блок 14) Сведения об условиях платежей</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:group ref="DealFulfillmentMain"/>
			<xs:element name="avgPayment" type="AvgPaymentType">
				<xs:annotation>
					<xs:documentation>Блок 29. Величина среднемесячного платежа по договору займа (кредита) и дата ее расчета</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="sourceNonMonetaryObligation" type="SourceNonMonetaryObligationType">
				<xs:annotation>
					<xs:documentation>Блок 30. (юр Блок 21) Сведения о неденежном обязательстве источника</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="participation" type="ParticipationType">
				<xs:annotation>
					<xs:documentation>Блок 56. (юр Блок 46) Сведения об участии в обязательстве, по которому формируется кредитная история</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DebtClaimSubmittedType">
		<xs:annotation>
			<xs:documentation>1.5 Для принудительного исполнения передано требование о взыскании долга по алиментам, платы за жилое помещение, коммунальные услуги или услуги связи</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:group ref="SubjectSpecialMain"/>
			<xs:element name="collection" type="CollectionType">
				<xs:annotation>
					<xs:documentation>Блок 43. (юр Блок 33) Сведения о взыскании долга по алиментам, платы за жилое помещение, коммунальные услуги или услуги связи</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="participation" type="ParticipationType">
				<xs:annotation>
					<xs:documentation>Блок 56. (юр Блок 46) Сведения об участии в обязательстве, по которому формируется кредитная история</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DebtClaimChangedType">
		<xs:annotation>
			<xs:documentation>1.6 Изменились сведения о взыскании долга по алиментам, платы за жилое помещение, коммунальные услуги или услуги связи либо указанное требование погашено</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="collection" type="CollectionType">
				<xs:annotation>
					<xs:documentation>Блок 43. (юр Блок 33) Сведения о взыскании долга по алиментам, платы за жилое помещение, коммунальные услуги или услуги связи</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="participation" type="ParticipationType">
				<xs:annotation>
					<xs:documentation>Блок 56. (юр Блок 46) Сведения об участии в обязательстве, по которому формируется кредитная история</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SubjectMainChangedType">
		<xs:annotation>
			<xs:documentation>1.7 и 1.8 Изменились сведения титульной части кредитной истории субъекта</xs:documentation>
		</xs:annotation>
		<xs:group ref="SubjectMain"/>
	</xs:complexType>
	<xs:complexType name="SubjectSpecialChangedType">
		<xs:annotation>
			<xs:documentation>1.9 Изменились сведения о субъекте в основной части кредитной истории, кроме сведений о дееспособности, банкротстве, индивидуальном рейтинге и кредитной оценке</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="regAddress" type="AddressType">
				<xs:annotation>
					<xs:documentation>Блок 8. Регистрация физического лица по месту жительства или пребывания</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="factAddress" type="AddressType">
				<xs:annotation>
					<xs:documentation>Блок 9. Фактическое место жительства</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="contact" type="ContactType">
				<xs:annotation>
					<xs:documentation>Блок 10. Контактные данные</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="selfEmploy" type="SelfEmployType">
				<xs:annotation>
					<xs:documentation>Блок 11. Государственная регистрация в качестве индивидуального предпринимателя</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SubjectCapacityChangedType">
		<xs:annotation>
